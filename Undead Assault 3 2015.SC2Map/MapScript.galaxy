    obj.keypos+= 1;
    block[1]^= obj.keysched[obj.keypos];
    obj.keypos+= 1;
    block[2]^= obj.keysched[obj.keypos];
    obj.keypos+= 1;
    block[3]^= obj.keysched[obj.keypos];
    obj.keypos+= 1;
