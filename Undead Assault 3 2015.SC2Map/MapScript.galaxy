void gt_InfestationLevelLoop_Init () {
    gt_InfestationLevelLoop = TriggerCreate("gt_InfestationLevelLoop_Func");
    TriggerAddEventTimePeriodic(gt_InfestationLevelLoop, 5.0, c_timeGame);
}

