    a = box[word & 0xFF];
    tc = box[(word >> 8) & 0xFF];
    a|= tc << 8;
    tc = box[(word >> 16) & 0xFF];
    a|= tc << 16;
    tc = box[(word >> 24) & 0xFF];
    a|= tc << 24;
    return a;
