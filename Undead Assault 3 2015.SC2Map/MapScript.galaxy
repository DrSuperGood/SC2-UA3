    unit lv_attacker;
    int lv_owner;
    string lv_unittype;
    int lv_chance;

    // Variable Initialization
    lv_attacker = EventUnit();
    lv_owner = EventPlayer();
    lv_unittype = UnitGetType(lv_attacker);
    lv_chance = (gv_magsize[lv_owner] + 1);

    // Conditions
    if (testConds) {
        if (!((UnitGroupHasUnit(gv_reloading, lv_attacker) == false))) {
