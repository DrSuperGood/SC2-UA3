        }

        if (!((UnitGroupHasUnit(gv_jammed, lv_attacker) == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    auto66AFEA78_val = lv_unittype;
    if (auto66AFEA78_val == "AutomaticRifleman") {
        if ((UnitHasBehavior(lv_attacker, "M135Minigun") == true)) {
            lv_chance *= 1024;
            lv_chance /= 499;
        }
        else {
            lv_chance *= 1024;
            lv_chance /= 424;
        }
    }
    else if (auto66AFEA78_val == "Cyborg") {
        if ((UnitHasBehavior(lv_attacker, "M135Minigun") == true)) {
            lv_chance *= 1024;
            lv_chance /= 499;
        }
        else {
            lv_chance *= 1024;
            lv_chance /= 424;
