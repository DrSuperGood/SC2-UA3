    block[1] = AESEncryptionSubWord(block[1], AES_ENCRYPTION_SBOX);
    block[2] = AESEncryptionSubWord(block[2], AES_ENCRYPTION_SBOX);
    block[3] = AESEncryptionSubWord(block[3], AES_ENCRYPTION_SBOX);
