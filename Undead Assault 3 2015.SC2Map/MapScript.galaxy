    int lv_failedshells;
    text lv_pluraltemp;

    // Variable Initialization
    lv_radius = gv_iDFRadius[EventPlayer()];
    lv_delaybasetime = 9.0;
    lv_distribution = gv_iDFDistro[EventPlayer()];
    lv_interval = gv_iDFInterval[EventPlayer()];
    lv_owner = UnitGetOwner(EventUnit());

